    <def-cat n="det-dem">
      <cat-item tags="det.dem.*"/>
    </def-cat>
    <def-cat n="adj">
      <cat-item tags="adj.*"/>
    </def-cat>
    <def-cat n="nom-pl-ind">
      <cat-item tags="n.pl.ind"/>
    </def-cat>
    <def-cat n="nom-eza">
      <cat-item tags="n.*.ezalos"/>
      <cat-item tags="n.*.ezaclo"/>
    </def-cat>
    <def-cat n="nom-sg-ind-eza">
      <cat-item tags="n.sg.ind.ezalos"/>
      <cat-item tags="n.sg.ind.ezaclo"/>
    </def-cat>
    <def-cat n="nom-pl-ind-eza">
      <cat-item tags="n.pl.ind.ezalos"/>
      <cat-item tags="n.pl.ind.ezaclo"/>
    </def-cat>
    <def-cat n="nom-sg-ind">
      <cat-item tags="n.sg.ind"/>
    </def-cat>
    <def-cat n="nom-abs">
      <cat-item tags="n.*.abs"/>
    </def-cat>
    <def-cat n="nom-abs-def">
      <cat-item tags="n.*.abs"/>
      <cat-item tags="n.*.def"/>
    </def-cat>
    <def-cat n="vbcop">
      <cat-item tags="vbcop.pri.*"/>
    </def-cat>
    <def-cat n="sent">
      <cat-item tags="sent"/>
    </def-cat>
  </section-def-cats>

  <section-def-attrs>
    <def-attr n="a_vbmod">
      <attr-item tags="vbmod"/>
    </def-attr>
    <def-attr n="a_prn">
      <attr-item tags="prn"/>
    </def-attr>
    <def-attr n="a_pers">
      <attr-item tags="pers"/>
    </def-attr>
    <def-attr n="a_nbr">
      <attr-item tags="pl"/>
      <attr-item tags="sg"/>
    </def-attr>
    <def-attr n="a_verb">
      <attr-item tags="vbser"/>
      <attr-item tags="vbcop"/>
      <attr-item tags="vblex"/>
    </def-attr>
    <def-attr n="a_poss">
      <attr-item tags="px1sg"/>
      <attr-item tags="px2sg"/>
      <attr-item tags="px3sg"/>
      <attr-item tags="px1pl"/>
      <attr-item tags="px2pl"/>
      <attr-item tags="px3pl"/>
    </def-attr>
    <def-attr n="a_clt">
      <attr-item tags="clt"/>
    </def-attr>
    <def-attr n="a_cas">
      <attr-item tags="nom"/>
    </def-attr>
    <def-attr n="a_tns">
      <attr-item tags="pri"/>
      <attr-item tags="pres"/>
    </def-attr>
    <def-attr n="a_tiv">
      <attr-item tags="tv"/>
      <attr-item tags="iv"/>
    </def-attr>
    <def-attr n="a_neg">
      <attr-item tags="neg"/>
    </def-attr>
    <def-attr n="a_pst">
      <attr-item tags="pst"/>
    </def-attr>
    <def-attr n="a_eza">
      <attr-item tags="ezaclo"/>
      <attr-item tags="ezalos"/>
    </def-attr>
    <def-attr n="a_prs">
      <attr-item tags="p1"/>
      <attr-item tags="p2"/>
      <attr-item tags="p3"/>
    </def-attr>
    <def-attr n="a_def">
      <attr-item tags="def"/>
      <attr-item tags="ind"/>
      <attr-item tags="abs"/>
      <attr-item tags="dem"/> <!-- CHECK -->
    </def-attr>
  </section-def-attrs>

  <section-def-vars>
    <def-var n="number"/>
    <def-var n="s_type"/>
    <def-var n="p_type"/>
    <def-var n="do_aux"/>
    <def-var n="determiner"/>
    <def-var n="agr"/>
    <def-var n="pronoun"/>
    <def-var n="cop"/>
    <def-var n="haver"/>
  </section-def-vars>

  <section-def-macros>

    <def-macro n="f_prep1" npar="1">
          <let><var n="p_type"/><clip pos="1" side="sl" part="lem"/></let>
     </def-macro>


    <def-macro n="f_sent1" npar="1">
          <let><var n="s_type"/><lit v="sent"/></let>
          <choose>
            <when>
              <test><equal caseless="yes"><clip pos="1" side="tl" part="lem"/><lit v="?"/></equal></test>
              <let><var n="s_type"/><lit v="qst"/></let>
            </when>
          </choose>
     </def-macro>

